<!DOCTYPE html>
<html>
<head>

    <!-- Meta -->
    <meta charset="utf-8">
    <meta name="keywords" content="mobile home insurance, mobile insurance, manufactured house insurance, mobile home insurance california" />
    <meta name="description" content="MobileHome.Insure is a place where you can purchase mobile home insurance in minutes. Get the right insurance policy at the right price!  Call us at 800-928-7244 or Email us at info@mobilehome.insure today.">
    <meta name="author" content="zachrichards.com">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Favicons -->
    <link rel="shortcut icon" href="~/content/img/favicon.ico">
    <link rel="apple-touch-icon" href="~/content/img/apple-touch-icon.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/content/img/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/content/img/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="144x144" href="~/content/img/apple-touch-icon-144x144.png">

    <!-- Google Webfonts -->
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,100,100italic,300,300italic,400italic,500,700,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,100,100italic,200,300,300italic,400italic,500,700,900' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,100,100italic,200,300,300italic,400italic,500,700,900' rel='stylesheet' type='text/css'>
    <title>@ViewBag.Title</title>


    

  @*  @Styles.Render("~/Content/css")*@
  @*  @Scripts.Render("~/bundles/modernizr")*@
    @Styles.Render("~/Content/css/bootstrapStyle")
    @Styles.Render("~/Content/Plugins/owl-carousel/carouselStyles")
    @Styles.Render("~/Content/css/CustomStyle")
    @Styles.Render("~/Content/font-awesome/css/fontstyle")
    @Styles.Render("~/Content/css/prettyPhoto")
    @Styles.Render("~/Content/Plugins/easytabs/cssStyles")
    @Styles.Render("~/Content/Plugins/flex-slider/sliderStyle")
    @Styles.Render("~/Content/Assets/Global/Plugins/select2/css")

</head>
<body id="page-top">
    <div id="dvLoading"></div>
    <!-- Outer-wrap -->
    <div class="outer-wrap">
        <div class="container">

            <!-- Fixed Left Navigaton -->
            <div class="col-md-3 m-left">
                @{Html.RenderPartial("~/Views/Shared/_menu.cshtml");}
            </div>

              @RenderBody()

        </div>
    </div>

  
    <script src="http://maps.google.com/maps/api/js?sensor=true"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrapscript")
    @Scripts.Render("~/Content/Plugins/pluginscripts")
    @Scripts.Render("~/bundles/Custom/customScripts")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Content/Assets/Global/Plugins/select2/scripts")

    @RenderSection("scripts", required: false)
</body>
</html>
