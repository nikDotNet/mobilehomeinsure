@model dynamic
@{
    Layout = null;
}
<div class="portlet-title"><div class="caption">Execute Quote</div></div>
<div class="portlet-body">
    <div class="row" data-animated="0">
        <div class="col-md-12" id="renterprint" style="margin-top:-25px;">
            <div id="m-info" class="center">
                <div id="page-header">
                    <h3><span>Insured Details</span></h3>
                </div>
                <dl class="dl-horizontal">
                    <dt>Insured Name</dt>
                    <dd id="infoName">@Model.infoName</dd>

                    <dt>Insured Address</dt>
                    <dd id="infoAddress1">@Model.infoAddress1</dd>

                    <dt>Insured Address</dt>
                    <dd id="infoAddress2">@Model.infoAddress2</dd>

                    <dt>Insured City</dt>
                    <dd id="infoCity">@Model.infoCity</dd>

                    <dt>Insured State</dt>
                    <dd id="infoState">@Model.infoState</dd>

                    <dt>Insured Zip Code</dt>
                    <dd id="infoZipCode">@Model.infoZipCode</dd>

                    <dt>Insured Phone</dt>
                    <dd id="infoPhone">@Model.infoPhone</dd>

                    <dt>Insured Email</dt>
                    <dd id="infoEmail">@Model.infoEmail</dd>
                </dl>
                <div id="page-header">
                    <h3><span>Transaction Details</span></h3>
                </div>
                <dl class="dl-horizontal">
                    <dt>Policy Number</dt>
                    <dd id="infopolnbr">@Model.infopolnbr</dd>

                    <dt>Insurance Company</dt>
                    <dd id="infocopcod">@Model.infocopcod</dd>

                    <dt>Mode Of Payment</dt>
                    <dd id="infomodeofpay">@Model.infomodeofpay</dd>

                    <dt>Payment ID</dt>
                    <dd id="infopmtid">@Model.infopmtid</dd>

                    <dt>Installment Plan</dt>
                    <dd id="infopayopt">@Model.infopayopt</dd>

                    <dt>Installment Fee</dt>
                    <dd id="infopmtinstfee">@Model.infopmtinstfee</dd>

                    <dt>Processing Fee</dt>
                    <dd id="infopmtprocfee">@Model.infopmtprocfee</dd>

                    <dt>Total Premium Amount</dt>
                    <dd id="infopmtamttotal">@Model.infopmtamttotal</dd>

                    <dt>Premium Charged Today</dt>
                    <dd id="infopmtamttoday">@Model.pmtamttoday</dd>

                    <dt>No. of Remaining Payments</dt>
                    <dd id="infonoofremainingpmt">@Model.infonoofremainingpmt</dd>

                    <dt>Transaction Date</dt>
                    <dd id="infotrndat">@Model.infotrndat</dd>

                    <dt>Transaction Time</dt>
                    <dd id="infotrntim">@Model.infotrntim</dd>
                </dl>
            </div>
        </div>

    </div>
    <div class="row">
        <div id="m-info" class="center">
            <div id="page-header">
                <h5><span>Enter Payment Details to Execute</span></h5>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="row field-row">
            <div class="col-xs-12 col-sm-4">
                @Html.LabelFor(x => x.NameOnCard)
                @Html.TextBoxFor(x => x.NameOnCard, new { @class = "le-input" })
                @Html.ValidationMessageFor(x => x.NameOnCard, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <button type="button" class="btn btn-default"
                    data-dismiss="modal">
                Cancel
            </button>
            <button type="submit" id="approve-btn"
                    class="btn btn-danger">
                Execute
            </button>
        </div>
    </div>

</div>