            <div class="page-sidebar navbar-collapse collapse">
                @{Html.RenderPartial("~/Areas/Admin/Views/Shared/_Menu.cshtml");}
            </div>
        </div>
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
